<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>jar\AutoFirma.jar</jar>
  <outfile>AutoFirma64\S-Sign.exe</outfile>
  <errTitle>Error: Se ha superado el tiempo de espera antes de recibir peticiones.</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>icono.ico</icon>
  <jre>
    <path>%JAVA_HOME%;%PATH%</path>
    <requiresJdk>false</requiresJdk>
    <requires64Bit>true</requires64Bit>
    <minVersion>1.8.0</minVersion>
    <maxVersion></maxVersion>
    <opt>-Xms512m -Xmx8g -Djdk.tls.maxHandshakeMessageSize=65536</opt>
  </jre>
  <splash>
    <file>splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>5</timeout>
    <timeoutErr>false</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>1.8.2.0</fileVersion>
    <txtFileVersion>1.8.2</txtFileVersion>
    <fileDescription>S-Sign</fileDescription>
    <copyright>Órgano judicial de Panamá</copyright>
    <productVersion>1.8.2.0</productVersion>
    <txtProductVersion>1.8.2</txtProductVersion>
    <productName>AutoFirma</productName>
    <companyName>Órgano judicial de Panamá</companyName>
    <internalName>S-Sign</internalName>
    <originalFilename>S-Sign.exe</originalFilename>
    <trademarks></trademarks>
    <language>CASTILIAN_SPANISH</language>
  </versionInfo>
</launch4jConfig>